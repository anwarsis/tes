<zk xmlns="http://www.zkoss.org/2005/zk/config"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.zkoss.org/2005/zk/config http://www.zkoss.org/2005/zk/config/zk.xsd">
<library-property>
    <name>org.zkoss.theme.preferred</name>
    <value>sapphire</value> <!-- silvertail   sapphire breeze  iceblue_c -->
</library-property>
 <library-property>
     <name>org.zkoss.zul.theme.fontSizeM</name>
     <value>13px</value>
 </library-property>
  <dependency>
            <groupId>org.zkoss.themepack</groupId>
            <artifactId>theme-pack</artifactId>
            <version>${zk.version}</version>
        </dependency>
 <library-property>
    <name>org.zkoss.zul.nativebar</name>
    <value>true</value>
</library-property>
    <session-config>
        <max-request-per-session>
            10000
        </max-request-per-session>
        <session-timeout>7200</session-timeout>
        <timer-keep-alive>false</timer-keep-alive>
        <device-type>ajax</device-type>
        <timeout-uri>
            http://192.168.0.202:8080/mr
        </timeout-uri>
    </session-config>
</zk>
